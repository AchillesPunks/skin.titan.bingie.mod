<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="movies">
    <name>Sci-Fi</name>
    <match>all</match>
    <rule field="genre" operator="contains">
        <value>SyFy</value>
        <value>Science Fiction</value>
        <value>Science-Fiction</value>
		<value>Sci-Fi</value>
        <value>SciFi</value>
    </rule>
    <order direction="descending">year</order>
    <limit>200</limit>
</smartplaylist>
