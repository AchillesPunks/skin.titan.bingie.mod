<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>TV Dramas</name>
    <match>all</match>
    <rule field="genre" operator="is">
        <value>Drama</value>
    </rule>
    <rule field="genre" operator="isnot">
        <value>Horror</value>
    </rule>
    <rule field="genre" operator="isnot">
        <value>Documentary</value>
    </rule>
    <order direction="descending">year</order>
	<limit>200</limit>
</smartplaylist>
