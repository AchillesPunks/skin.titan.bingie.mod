<smartplaylist type="tvshows">
    <name>TV War &amp; Politics</name>
    <match>all</match>
    <rule field="genre" operator="contains">       
        <value>War</value>
		<value>Politics</value>
    </rule>
    <order direction="descending">year</order>
    <limit>200</limit>
</smartplaylist>
