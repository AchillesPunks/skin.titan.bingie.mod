<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Quick Hits</name>
    <match>all</match>
    <rule field="time" operator="lessthan">
        <value>01:35:00</value>
    </rule>
    <rule field="rating" operator="greaterthan">
        <value>6.3</value>
    </rule>
    <rule field="votes" operator="greaterthan">
        <value>500</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
