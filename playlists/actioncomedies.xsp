<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Action Comedies</name>
    <match>all</match>
    <rule field="genre" operator="is">
        <value>Action</value>
    </rule>
    <rule field="genre" operator="is">
        <value>Comedy</value>
    </rule>
    <rule field="tag" operator="isnot">
        <value>superhero</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
