<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>War Movies</name>
    <match>all</match>
    <rule field="genre" operator="is">
        <value>War</value>
    </rule>
    <rule field="votes" operator="greaterthan">
        <value>100</value>
    </rule>
    <rule field="genre" operator="isnot">
        <value>Documentary</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
