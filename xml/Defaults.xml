<?xml version="1.0" encoding="utf-8"?>
<!-- BINGIE -->
<includes>
    <default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <label/>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(GeneralTextColor)]</focusedcolor>
        <selectedcolor>$INFO[Skin.String(GeneralTextColor)]</selectedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <scroll>false</scroll>
        <scrollspeed>30</scrollspeed>
    </default>
    <default type="fadelabel">
        <posx>0</posx>
        <posy>0</posy>
        <label/>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(GeneralTextColor)]</focusedcolor>
        <selectedcolor>$INFO[Skin.String(GeneralTextColor)]</selectedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <scrollspeed>30</scrollspeed>
        <pauseatend>2000</pauseatend>
        <scrollout>false</scrollout>
    </default>
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width/>
        <height/>
        <align>center</align>
        <aligny>center</aligny>
        <texture/>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="button">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <label/>
        <textcolor fallback="white">$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
        <textoffsetx>50</textoffsetx>
    </default>
	
	<default type="colorbutton">
        <align>center</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <textoffsetx>3</textoffsetx>
        <textcolor fallback="white">$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
		<disabledcolor>aaffffff</disabledcolor>
        <colorwidth>140</colorwidth>
        <colorheight>26</colorheight>
		<texturecolormask>diffuse/colorpicker.png</texturecolormask>
		<texturecolordisabledmask colordiffuse="aaffffff">diffuse/colorpicker.png</texturecolordisabledmask>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
        <pulseonselect>false</pulseonselect>
    </default>
	
	<default type="edit">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <label/>
        <textcolor>$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <pulseonselect>false</pulseonselect>
		<texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <textoffsetx>50</textoffsetx>
		<textoffsety>8</textoffsety>
    </default>
    <default type="togglebutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <label/>
        <font>Light28</font>
        <textcolor>$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <align>center</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
        <alttexturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</alttexturefocus>
        <alttexturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</alttexturenofocus>
        <textoffsetx>50</textoffsetx>
    </default>
    <default type="radiobutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <radiowidth>120</radiowidth>
        <radioheight>60</radioheight>
        <align>center</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <label/>
        <textcolor>$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textureradiofocus colordiffuse="$INFO[Skin.String(ActiveSpinControlColor)]">common/radio/radio-button-fo.png</textureradiofocus>
        <textureradionofocus colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/radio/radio-button-nf.png</textureradionofocus>
        <textureradioondisabled colordiffuse="aaffffff">common/radio/radio-button-fo.png</textureradioondisabled>
        <textureradiooffdisabled colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/radio/radio-button-nf.png</textureradiooffdisabled>
        <textoffsetx>50</textoffsetx>
        <radioposx/>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
    </default>
    <default type="spincontrolex">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <label/>
        <font>Light28</font>
        <textureup colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/spinctrl/arrow-up-nf.png</textureup>
        <texturedown colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/spinctrl/arrow-down-nf.png</texturedown>
        <textureupdisabled colordiffuse="aaffffff">common/spinctrl/arrow-up-nf.png</textureupdisabled>
        <texturedowndisabled colordiffuse="aaffffff">common/spinctrl/arrow-down-nf.png</texturedowndisabled>
        <textureupfocus colordiffuse="$INFO[Skin.String(ActiveSpinControlColor)]">common/spinctrl/arrow-up-nf.png</textureupfocus>
        <texturedownfocus colordiffuse="$INFO[Skin.String(ActiveSpinControlColor)]">common/spinctrl/arrow-down-nf.png</texturedownfocus>
        <spinwidth>40</spinwidth>
        <spinheight>60</spinheight>
        <textcolor>$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textoffsetx>50</textoffsetx>
        <align>center</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
    </default>
    <default type="spincontrol">
        <posx>0</posx>
        <posy>0</posy>
        <width>23</width>
        <height>23</height>
        <textureup colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/spinctrl/arrow-up-nf.png</textureup>
        <texturedown colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/spinctrl/arrow-down-nf.png</texturedown>
        <textureupdisabled colordiffuse="aaffffff">common/spinctrl/arrow-up-nf.png</textureupdisabled>
        <texturedowndisabled colordiffuse="aaffffff">common/spinctrl/arrow-down-nf.png</texturedowndisabled>
        <textureupfocus colordiffuse="$INFO[Skin.String(ActiveSpinControlColor)]">common/spinctrl/arrow-up-nf.png</textureupfocus>
        <texturedownfocus colordiffuse="$INFO[Skin.String(ActiveSpinControlColor)]">common/spinctrl/arrow-down-nf.png</texturedownfocus>
        <spinwidth>40</spinwidth>
        <spinheight>60</spinheight>
        <font>Light28</font>
        <textcolor>$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textoffsetx>50</textoffsetx>
        <align>center</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
    </default>
    <default type="sliderex">
        <posx>0</posx>
        <posy>0</posy>
        <height>60</height>
        <width>450</width>
        <sliderwidth>210</sliderwidth>
        <sliderheight>60</sliderheight>
        <orientation>horizontal</orientation>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="10">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(ButtonColor)]" border="10">diffuse/panel.png</texturenofocus>
        <texturesliderbar border="5" colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/modern/sliderex/slider.png</texturesliderbar>
        <textureslidernib border="5" colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/modern/sliderex/nib.png</textureslidernib>
        <textureslidernibfocus border="5" colordiffuse="$INFO[Skin.String(ActiveSpinControlColor)]">common/modern/sliderex/nib.png</textureslidernibfocus>
        <aligny>center</aligny>
        <font>Light28</font>
        <textcolor>$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textoffsetx>50</textoffsetx>
        <textoffsety>0</textoffsety>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="progress">
        <posx>0</posx>
        <posy>0</posy>
        <width>280</width>
        <height>20</height>
        <info/>
        <texturebg border="10" colordiffuse="$INFO[Skin.String(OSDPanelColor)]">diffuse/osd_panel.png</texturebg>
		<lefttexture/>
		<midtexture border="10" colordiffuse="$INFO[Skin.String(ViewDetailsHighlightTextColor)]">diffuse/osd_panel.png</midtexture>
        <righttexture/>
        <overlaytexture/>
    </default>
    <default type="slider">
        <posx>0</posx>
        <posy>0</posy>
        <width>300</width>
        <height>20</height>
        <orientation>horizontal</orientation>
        <texturesliderbar border="5" colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/modern/slider/slider.png</texturesliderbar>
        <textureslidernib border="5" colordiffuse="$INFO[Skin.String(ButtonTextColor)]">common/modern/slider/nib_focus.png</textureslidernib>
        <textureslidernibfocus border="5" colordiffuse="$INFO[Skin.String(ActiveSpinControlColor)]">common/modern/slider/nib_focus.png</textureslidernibfocus>
    </default>
    <default type="scrollbar">
        <width>20</width>
        <texturesliderbackground border="10" colordiffuse="$INFO[Skin.String(ScrollbarBackgroundColor)]">scrollbar/texturesliderbar.png</texturesliderbackground>
		<texturesliderbar colordiffuse="$INFO[Skin.String(ScrollbarUnfocusedColor)]" border="10">scrollbar/texturesliderbar.png</texturesliderbar>
		<texturesliderbarfocus colordiffuse="$INFO[Skin.String(ScrollbarFocusedColor)]" border="10">scrollbar/texturesliderbar.png</texturesliderbarfocus>
		<textureslidernib colordiffuse="$INFO[Skin.String(ScrollbarUnfocusedColor)]">scrollbar/texturesliderbar_nib.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$INFO[Skin.String(ScrollbarFocusedColor)]">scrollbar/texturesliderbar_nib.png</textureslidernibfocus>
		<showonepage>false</showonepage>
        <orientation>vertical</orientation>
        <visible>!String.IsEqual(Skin.String(scrollbar_visibility),always_hide)</visible>
    </default>
    <default type="textbox">
        <label/>
        <font>Light28</font>
        <textcolor fallback="white">$INFO[Skin.String(GeneralTextColor)]</textcolor>
        <shadowcolor/>
        <pagecontrol>61</pagecontrol>
		<align>justify</align>
        <autoscroll delay="8000" time="3000" repeat="10000">!Skin.HasSetting(DisablePlotAutoScroll)</autoscroll>
    </default>
</includes>
